using namespace std;

Map::Map(const string fileName)
{
  ifstream g(filename);
  while(g.good())
    {
      g >> title;
      g.get();
      cout << title<< endl;
      getline(g, def);
      g.get();
      entries.insert(StrStrpair(title, def));
    }
  g.close();
}

